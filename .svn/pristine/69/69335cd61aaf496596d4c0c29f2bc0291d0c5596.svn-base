= <h3>Database management</h3>
src/database/DataGenerate.java = generate dump information for testing
src/database/DataGenerateTest.java = test DataGenerate
src/database/DateCheck.java = check input date
src/database/DateCheckTest.java = test for DateCheck
=
= <h3>Data management classes</h3>
src/bookings/Client.java = control client registration
src/bookings/ClientTest.java = control client registration test
src/bookings/ClientsBase.java = control clients 
src/bookings/CalendarView.java = handle bases to deliver info on request 
src/bookings/Reservation.java = control resrvation registration
src/bookings/ReservationTest.java = control reservation registration  test
src/bookings/ReservationsBase.java = control resrvations
src/bookings/Room.java = control room registration
src/bookings/RoomTest.java = test room registration
src/bookings/RoomsBase.java = control rooms
src/bookings/RoomsBaseTest.java = test control rooms
src/bookings/HoldException.java = constructor for exceptions
=
